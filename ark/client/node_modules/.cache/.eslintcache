r\n    }\r\n\r\n    // This method will map out the records on the table\r\n    function recordList() {\r\n        return records.map((record) => {\r\n            return (\r\n                <div class=\"grid-item\">\r\n                    <Record\r\n                        record={record}\r\n                        deleteRecord={() => deleteRecord(record._id)}\r\n                        key={record._id}\r\n                    />\r\n                </div>\r\n            );\r\n        });\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <h1>BIG SCARRY BOSSS</h1>\r\n\r\n\r\n            <div class=\"grid-container\">\r\n\r\n                {recordList()}\r\n\r\n                <div class=\"grid-item\"><BossCard name=\"Rebecca\"></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                {/*One item in the grid not a bunch of items */}\r\n\r\n            </div>\r\n\r\n\r\n\r\n        </div>\r\n\r\n    );\r\n}\r\n\r\n\r\n","E:\\Uni\\Ark_Software\\ark\\client\\src\\components\\editUser.js",[],[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":15,"column":9,"nodeType":"69","endLine":15,"endColumn":214},{"ruleId":"70","severity":1,"message":"71","line":3,"column":10,"nodeType":"72","messageId":"73","endLine":3,"endColumn":14},"no-native-reassign",["74"],"no-negated-in-lhs",["75"],"jsx-a11y/alt-text","img elements must have an alt prop, either with mea