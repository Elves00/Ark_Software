 }, [records.length]);\r\n\r\n\r\n    // This method will delete a record\r\n    async function deleteRecord(id) {\r\n        await fetch(`http://localhost:5000/${id}`, {\r\n            method: \"DELETE\"\r\n        });\r\n\r\n        const newRecords = records.filter((el) => el._id !== id);\r\n        setRecords(newRecords);\r\n    }\r\n\r\n    // This method will map out the records on the table\r\n    function recordList() {\r\n        return records.map((record) => {\r\n            return (\r\n                <div class=\"grid-item\">\r\n                    <Record\r\n                        record={record}\r\n                        deleteRecord={() => deleteRecord(record._id)}\r\n                        key={record._id}\r\n                    />\r\n                </div>\r\n            );\r\n        });\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <h1>BIG SCARRY BOSSS</h1>\r\n\r\n\r\n            <div class=\"grid-container\">\r\n\r\n                {recordList()}\r\n\r\n                <div class=\"grid-item\"><BossCard name=\"Rebecca\"></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                {/*One item in the grid not a bunch of items */}\r\n\r\n            </div>\r\n\r\n\r\n\r\n        </div>\r\n\r\n    );\r\n}\r\n\r\n\r\n","C:\\Users\\rebec\\OneDrive - AUT University\\Software Team Project\\Ark_Software\\ark\\client\\src\\components\\editUser.js",[],"C:\\Users\\rebec\\OneDrive - AUT University\\