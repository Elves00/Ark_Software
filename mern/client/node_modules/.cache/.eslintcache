Delete\r\n        </button>\r\n    </div>\r\n);\r\n\r\nexport default function Boss() {\r\n\r\n    //useState sets the function state\r\n    const [records, setRecords] = useState([]);\r\n\r\n    // This method fetches the records from the database.\r\n    useEffect(() => {\r\n        async function getRecords() {\r\n            const response = await fetch(`http://localhost:5000/record/`);\r\n\r\n            if (!response.ok) {\r\n                const message = `An error occured: ${response.statusText}`;\r\n                window.alert(message);\r\n                return;\r\n            }\r\n\r\n            const records = await response.json();\r\n            setRecords(records);\r\n        }\r\n\r\n        getRecords();\r\n\r\n        return;\r\n    }, [records.length]);\r\n\r\n\r\n    // This method will delete a record\r\n    async function deleteRecord(id) {\r\n        await fetch(`http://localhost:5000/${id}`, {\r\n            method: \"DELETE\"\r\n        });\r\n\r\n        const newRecords = records.filter((el) => el._id !== id);\r\n        setRecords(newRecords);\r\n    }\r\n\r\n    // This method will map out the records on the table\r\n    function recordList() {\r\n        return records.map((record) => {\r\n            return (\r\n                <div class=\"grid-item\">\r\n                    <Record\r\n                        record={record}\r\n                        deleteRecord={() => deleteRecord(record._id)}\r\n                        key={record._id}\r\n                    />\r\n                </div>\r\n            );\r\n        });\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <h1>BIG SCARRY BOSSS</h1>\r\n\r\n\r\n            <div class=\"grid-container\">\r\n\r\n                {recordList()}\r\n\r\n                <div class=\"grid-item\"><BossCard name=\"Rebecca\"></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                <div class=\"grid-item\"><BossCard></BossCard></div>\r\n                {/*One item in the grid not a bunch of items */}\r\n\r\n            </div>\r\n\r\n\r\n\r\n        </div>\r\n\r\n    );\r\n}\r\n\r\n\r\n","E:\\Uni\\Software Project\\mern-stack-example\\mern\\client\\src\\components\\bossCard.js",[],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\index.js",[],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\App.js",[],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\components\\recordList.js",[],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\components\\navbar.js",["98"],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\components\\create.js",[],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\components\\editUser.js",[],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\components\\createUser.js",[],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\components\\recordUserList.js",[],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\components\\edit.js",[],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\components\\bossPage.js",["99"],"E:\\Uni\\Software Project\\mern-stack-example - Copy\\mern\\client\\src\\components\\bossCard.js",[],{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":15,"column":9,"nodeType":"106","endLine":15,"endColumn":214},{"ruleId":"107","severity":1,"me